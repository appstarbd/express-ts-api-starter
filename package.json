{
  "name": "express-ts-api-starter",
  "version": "1.0.0",
  "description": "API Starter kit for Express in TypeScript with Paseto authentication",
  "main": "index.js",
  "scripts": {
    "start": "node dist/server.js",
    "compile": "babel src --out-dir dist --extensions '.js,.jsx,.ts,.tsx' --copy-files",
    "dev": "nodemon src/server.ts --exec babel-node --presets @babel/env,@babel/typescript --extensions '.js,.jsx,.ts,.tsx' --config .nodemonrc.json | pino-pretty",
    "dev:debug": "nodemon src/server.ts --exec babel-node --config .nodemonrc.json --inspect | pino-pretty",
    "lint": "eslint --ext=.ts,.js src/",
    "lint:fix": "eslint --fix --ext=.ts,.js src/",
    "check:types": "tsc --noEmit",
    "test": "jest --verbose=true --forceExit",
    "configure": "babel-node --presets @babel/env,@babel/typescript --extensions '.js,.jsx,.ts,.tsx' console/configure.js"
  },
  "dependencies": {
    "bcrypt": "^3.0.4-napi",
    "body-parser": "^1.18.3",
    "commander": "^2.20.0",
    "cors": "^2.8.5",
    "db-errors": "^0.1.1",
    "dotenv": "^6.2.0",
    "email-templates": "^5.0.4",
    "envfile": "^3.0.0",
    "esm": "^3.2.22",
    "express": "^4.16.4",
    "faker": "^4.1.0",
    "i18n": "^0.8.3",
    "is-email": "^1.0.0",
    "json-api-serializer": "^1.15.1",
    "knex": "^0.16.3",
    "morgan": "^1.9.1",
    "nodemailer": "^5.1.1",
    "objection": "^1.6.3",
    "paseto.js": "https://github.com/stefanvanherwijnen/paseto.js",
    "pino": "^5.11.1",
    "pug": "^2.0.3",
    "randomstring": "^1.1.5",
    "sqlite3": "^4.0.6",
    "superstruct": "^0.6.0",
    "swagger-jsdoc": "^3.2.8",
    "swagger-ui-express": "^4.0.2",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-typescript": "^7.3.3",
    "@types/express": "^4.11.0",
    "@types/jest": "^24.0.11",
    "@types/knex": "^0.14.5",
    "@types/morgan": "^1.7.35",
    "@types/node": "^11.13.0",
    "@types/nodemailer": "^4.6.6",
    "@typescript-eslint/eslint-plugin": "^1.5.0",
    "@typescript-eslint/parser": "^1.5.0",
    "babel-jest": "^24.5.0",
    "eslint": "^5.15.3",
    "jest": "^24.5.0",
    "jest-environment-node": "^24.5.0",
    "nodemon": "^1.18.10",
    "pino-pretty": "^2.5.0",
    "supertest": "^4.0.2",
    "typescript": "^3.3.4000"
  }
}
